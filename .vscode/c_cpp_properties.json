{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "${default}",
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "includePath": [
                "/home/alm/avr-code/mega/inc/**",
                "/usr/lib/avr/include/**",
                "/usr/lib/avr/include/avr/**"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-ffunction-sections",
                "-fdata-sections",
                "-pipe",
                "-mmcu=atmega128a",
                "-DF_CPU=8000000UL"
            ]
        }
    ],
    "version": 4
}